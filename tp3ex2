#include <stdio.h>

void afficherListe(int L[], int taille) {
    for (int i = 0; i < taille; i++) {
        printf("%d\n", L[i]);tp3
    }
}

int main() {
    int ma_liste[] = {1, 2, 3, 4, 5};
    int taille = sizeof(ma_liste) / sizeof(ma_liste[0]);

    afficherListe(ma_liste, taille);

    return 0;
}
